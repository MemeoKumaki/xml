			<mets:xmlData>
				<mods:mods version="3.4">
					<mods:titleInfo>
						<mods:title>Great conversations: the pianists</mods:title>
					</mods:titleInfo>
					<mods:name>
						<mods:namePart>Istomin, Eugene</mods:namePart>
						<mods:role>
							<mods:roleTerm authority="marcrelator" type="text">Host</mods:roleTerm>
